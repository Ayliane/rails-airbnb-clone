<div class = "container text-center ">
    <div class = "row">
      <div class = "col-xs-12 col-sm-offset-4 col-sm-4 ">
        <div class = 'registration-box'>
          <%= simple_form_for(@user) do |f| %>
            <%= f.input :first_name %>
            <%= f.input :last_name %>
            <%= f.input :email %>
            <%= f.input :dog_name %>
            <%= f.input :photo, as: :attachinary %>
            <%= f.submit "edit", class: "login-button" %>
          <% end %>
          </div>
        </div>
      </div>
    </div>
