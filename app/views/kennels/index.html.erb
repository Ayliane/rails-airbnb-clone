<div class="container">
  <div class="row">
    <div class="col-xs-12">

    <h2>Available kennels in <%= params[:city].capitalize %></h2>

    <ul>
      <% @kennels.each do |kennel| %>
        <li><%= link_to kennel.description, kennel_path(kennel[:id]) %></li>
      <% end %>
    </ul>
    </div>
  </div>
</div>
